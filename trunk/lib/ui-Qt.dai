;;; ui-Qt.dai --- Logs for communications with the Qt GUI library.

(deflog QtTime Qt
  "Let Qt UI know the simulation time."
  (when (or (hourly 7) finished))
  (entries (number (path time year))
           (number (path time month))
           (number (path time mday))
           (number (path time hour))
           (number (path time minute))
           (number (path time second))))

;;; ui-Qt.dai ends here.
