  (CrpN (PtLeafCnc (0.00 0.060) (1.00 0.050) (1.30 0.040) (1.50 0.025) (2.00 0.020))
        (CrLeafCnc (0.00 0.045) (1.00 0.040) (1.30 0.030) (1.50 0.020) (2.00 0.010))
