          (nitrogen_stress_limit 0.01))
  (Canopy (SpLAI  0.020 [(m^2/m^2)/(g DM/m^2)])
          (LeafAIMod (1.5 1.0) (2.0 0.3))
          (DSLAI05   0.2)
          (SpLAIfac (0.0 100) (1.0 100)))
  (Prod (LfDR (0.00 0.00) (1.50 0.00) (1.60 0.02) (2.00 0.10))
