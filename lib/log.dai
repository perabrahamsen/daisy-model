(deflog "Soil Chemicals" chemical
  "Summary information about chemicals in the soil."
  (where "soil_chemical.dlf")
  (summary (balance (precision 3)
                    (input "In" "External" "Sorption" "Transform")
                    (output "Leak-Matrix" "Leak-Macro" "Drain" 
                            "Uptake")
                    (content "Content")))
  (when hourly)
  (entries ;; Year Month MDay Hour
           (flux_top (path column "${column}" Chemistry "*" 
                           trace "${chemical}" J)
                     (tag "In")
                     (handle sum)
                     (negate true)
                     (dimension "g/ha/&")
                     (spec chemical default J))
           (flux_bottom (path column "${column}" Chemistry "*" 
                              trace "${chemical}" J)
                        (tag "Leak-Matrix")
                        (negate true)
                        (handle sum)
                        (dimension "g/ha/&")
                        (spec chemical default J))
           (flux_bottom (path column "${column}" Chemistry "*" 
                              trace "${chemical}" J_p)
                        (tag "Leak-Macro")
                        (negate true)
                        (handle sum)
                        (dimension "g/ha/&")
                        (spec chemical default J_p))
           (interval (path column "${column}" Chemistry "*" 
                           trace "${chemical}" S_drain)
                     (tag "Drain")
                     (negate true)
                     (handle sum)
                     (dimension "g/ha/&")
                     (spec chemical default S_drain))
           (interval (path column "${column}" Chemistry "*" 
                           trace "${chemical}" S_external)
                     (tag "External")
                     (handle sum)
                     (dimension "g/ha/&")
                     (spec chemical default S_external))
           (interval (path column "${column}" Chemistry "*" 
                           trace "${chemical}" S_root)
                     (tag "Uptake")
                     (handle sum)
		     (negate true)
                     (dimension "g/ha/&")
                     (spec chemical default S_root))
           (interval (path column "${column}" Chemistry "*" 
                           trace "${chemical}" S_sorption)
                     (tag "Sorption")
                     (handle sum)
                     (dimension "g/ha/&")
                     (spec chemical default S_sorption))
           (interval (path column "${column}" Chemistry "*" 
                           trace "${chemical}" S_transform)
                     (tag "Transform")
                     (handle sum)
                     (dimension "g/ha/&")
                     (spec chemical default S_transform))
           (interval (path column "${column}" Chemistry "*" 
                           trace "${chemical}" M)
                     (tag "Content")
                     (dimension "g/ha")
                     (spec chemical default M))))

