(deflog "Surface Chemicals" chemical
  "Summary information about a specific chemical on the soil surface.
Use like (\"Surface Chemicals\" (chemical Atrazine)) to log the 
content of 'Atrazine' on the soil.  If you don't specify chemical, the 
sum of all chemicals will be logged."
  (where "${colfid}surface_chemicals.dlf")
  (declare unit String "Base unit.")
  (unit "g/ha")
  (summary (balance (description "Snow")
		    (precision 3)
                    (input snow_in)
                    (output snow_out)
                    (content snow_storage ))
	   (balance (description "Canopy")
		    (precision 3)
                    (input canopy_in canopy_transform)
                    (output canopy_harvest canopy_dissipate canopy_out)
                    (content canopy_storage))
	   (balance (description "Litter")
		    (precision 3)
                    (input litter_in litter_transform)
                    (output litter_decompose
			    litter_leak litter_diffuse litter_tillage)
                    (content litter_storage))
	   (balance (description "Surface")
		    (precision 3)
                    (input surface_in surface_transform)
                    (output surface_decompose 
                            surface_runoff surface_out surface_mixture 
                            surface_tillage)
                    (content surface_solute surface_immobile))
	   (balance (description "All")
		    (precision 3)
                    (input spray deposit surface_transform litter_transform)
                    (output canopy_harvest canopy_dissipate
			    litter_decompose surface_decompose 
                            surface_runoff surface_out surface_mixture 
                            litter_tillage surface_tillage)
                    (content snow_storage canopy_storage litter_storage
                             surface_solute surface_immobile)))
