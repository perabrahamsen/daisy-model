           (index (path column "${column}" Vegetation crops crops "${crop}"
                        "${fraction}" "*" cs_vector)
                  (index $layer)
                  (spec photosynthesis Farquhar cs_vector)
                  (handle current))
           (number (dimension "ng/cm^3")
		   (path column "${column}" 
			 Vegetation crops crops "${crop}" Root ABAConc)
		   (spec fixed RootSystem ABAConc)
                   (handle current))
           (number (path column "${column}" 
			 Vegetation crops crops "${crop}" Root h_x)
		   (spec fixed RootSystem h_x)
                   (handle current))
           (number (path column "${column}" Vegetation crops crops "${crop}"
                         "${fraction}" "*" ABA_effect)
                   (spec photosynthesis Farquhar ABA_effect)
                   (handle current))
           (number (path column "${column}" Vegetation crops crops "${crop}"
                         "${fraction}" "*" Gamma)
                   (spec photosynthesis Farquhar Gamma)
                   (handle current))
           (number (path column "${column}" Vegetation crops crops "${crop}"
                         "${fraction}" "*" gbw)
                   (spec photosynthesis Farquhar gbw)
                   (handle current))
           (number (path column "${column}" Vegetation crops crops "${crop}"
                         "${fraction}" "*" Ass)
                   (spec photosynthesis Farquhar Ass)
                   (handle sum))))

(deflog "Time" column
  "Keep track of time."
  (where "time.dlf")
  (summary (simple (fetch (Used) (SoilWater))))
  (when true)
  (entries (number (path dt)
                   (tag "Used")
                   (handle average)
                   (spec program Daisy dt))
           (number (path column "${column}" SoilWater dt)
                   (tag "SoilWater")
                   (handle average)
                   (spec fixed SoilWater dt))
           (number (path column "${column}" SoilWater sink_cell)
                   (tag "SW Cell")
                   (handle average)
                   (spec fixed SoilWater sink_cell))
           (number (tag "NO3")
                   (path column "${column}" Chemistry multi trace NO3 dt)
                   (spec chemical default dt))
           (number (tag "NO3 Cell")
                   (path column "${column}" Chemistry multi trace NO3 sink_cell)
                   (spec chemical default sink_cell))
           ))

(deflog "Drain lateral" column
  "Keep track of lateral drains."
  (where "drain_lateral.dlf")
  (when true)
  (entries (number (path column "${column}" Drain lateral pipe_level)
                   (spec drain lateral pipe_level)
                   (handle current))
           (number (path column "${column}" Drain lateral height)
                   (spec drain lateral height)
                   (handle current))
           (number (path column "${column}" Drain lateral DrainFlow)
                   (spec drain lateral DrainFlow)
                   (handle sum))
           (number (path column "${column}" Drain lateral EqDrnFlow)
                   (spec drain lateral EqDrnFlow)
                   (handle sum))
           (volume (path column "${column}" Drain lateral S)
                   (spec drain lateral S)
                   (handle sum))))

(deflog biopore-chemical chemical
  "Log chemicals in biopores."
  (declare biopore String
	   "Name of biopore to log.")
  (biopore "*")
  ;; Add it to the log files.
  (parameter_names &old biopore)
  ;; Use chemical name as part of file name.
  (declare bioporefid string
           "File name component indicating biopore logged.")
  (bioporefid (cond ((string-equal "${biopore}" "*")
		"biopore")
	       (true
		"${biopore}"))))
  
(defsummary "Biopore content" balance
  (precision 3)
  (input "From matrix"
	 "From above")
  (output "To below" "To drain")
  (content "Content"))

(deflog "Biopore chemical" biopore-chemical
  (declare unit String "Base unit.")
  (unit "g/ha")
  (where "${colfid}tertiary-${bioporefid}-${chemfid}.dlf")
  (when hourly)
  (summary "Biopore content")
  (entries (interval (path column "${column}" Movement "*"
			   Tertiary biopores classes "${biopore}"
			   M "${chemical}" value)
		     (handle current)
		     (tag "Content")
		     (dimension "${unit}")
		     (spec biopore matrix M value))
	   (interval (path column "${column}" Movement "*"
			   Tertiary biopores classes "${biopore}"
			   S_chem "${chemical}" value)
		     (handle sum)
		     (tag "From matrix")
		     (dimension "${unit}")
		     (spec biopore matrix S_chem value))
	   (interval (path column "${column}" Movement "*"
			   Tertiary biopores classes "${biopore}"
			   S_chem_to_drain "${chemical}" value)
		     (handle sum)
		     (tag "To drain")
		     (dimension "${unit}")
		     (spec biopore drain S_chem_to_drain value))
           (flux_top (tag "From above")
                     (path column "${column}"  Movement "*"
			   Tertiary biopores classes "${biopore}"
			   J "${chemical}" value)
		     (dimension "${unit}")
                     (spec biopore matrix J value)
                     (handle sum)
                     (negate true))
           (flux_bottom (tag "To below")
			(path column "${column}"  Movement "*"
			      Tertiary biopores classes "${biopore}"
			      J "${chemical}" value)
			(dimension "${unit}")
			(spec biopore matrix J value)
			(handle sum)
			(negate true))
	   ))

(deflog "Biopore water" biopore
  (declare unit String "Base unit.")
  (unit "mm")
  (where "${colfid}tertiary-${bioporefid}-water.dlf")
  (when hourly)
  (summary "Biopore content")
  (entries (interval (path column "${column}" Movement "*"
			   Tertiary biopores classes "${biopore}" Theta)
		     (handle current)
		     (tag "Content")
		     (dimension "${unit}")
		     (spec biopore matrix Theta))
	   (interval (path column "${column}" Movement "*"
			   Tertiary biopores classes "${biopore}"
			   S)
		     (handle sum)
		     (tag "From matrix")
		     (dimension "${unit}")
		     (spec biopore matrix S))
	   (interval (path column "${column}" Movement "*"
			   Tertiary biopores classes "${biopore}"
			   S_to_drain)
		     (handle sum)
		     (tag "To drain")
		     (dimension "${unit}")
		     (spec biopore drain S_to_drain))
           (flux_top (tag "From above")
                     (path column "${column}"  Movement "*"
			   Tertiary biopores classes "${biopore}"
			   q)
                     (dimension "${unit}")
                     (spec biopore component q)
		     (handle sum)
                     (negate true))
           (flux_bottom (tag "To below")
			(path column "${column}"  Movement "*"
			      Tertiary biopores classes "${biopore}"
			      q)
			(dimension "${unit}")
			(spec biopore component q)
			(handle sum)
			(negate true))
	   ))

